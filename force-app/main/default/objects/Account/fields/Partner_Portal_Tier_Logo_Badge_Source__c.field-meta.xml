<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Partner_Portal_Tier_Logo_Badge_Source__c</fullName>
    <description>Text formula to display dynamic source path for tier logo badge display.</description>
    <externalId>false</externalId>
    <formula>CASE(TEXT(Experts_Tier__c),
&quot;Silver&quot;, &quot;/sfsites/c/resource/PartnerPortalLogoBadgeSilverMaster&quot;,
&quot;Gold&quot;, &quot;/sfsites/c/resource/PartnerPortalLogoBadgeGoldMaster&quot;,
&quot;Platinum&quot;, &quot;/sfsites/c/resource/PartnerPortalLogoBadgePlatinumMaster&quot;,
&quot;Elite&quot;, &quot;/sfsites/c/resource/PartnerPortalLogoBadgeEliteMaster&quot;,
&quot;Klaviyo Partner&quot;, &quot;/sfsites/c/resource/PartnerPortalLogoBadgeKlaviyoPartner&quot;,
CASE(TEXT(Services_Tier__c),
&quot;Silver&quot;, &quot;/sfsites/c/resource/PartnerPortalLogoBadgeSilverAdvisor&quot;,
&quot;Gold&quot;, &quot;/sfsites/c/resource/PartnerPortalLogoBadgeGoldAdvisor&quot;,
&quot;Platinum&quot;, &quot;/sfsites/c/resource/PartnerPortalLogoBadgePlatinumAdvisor&quot;,
&quot;Elite&quot;, &quot;&quot;,
&quot;Klaviyo Partner&quot;, &quot;/sfsites/c/resource/PartnerPortalLogoBadgeKlaviyoPartner&quot;,
&quot;&quot;)
)</formula>
    <label>Partner Portal Tier Logo Badge Source</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
